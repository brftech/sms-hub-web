{
  "buildCommand": "pnpm build --filter @sms-hub/unified",
  "outputDirectory": "apps/unified/dist",
  "installCommand": "pnpm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/((?!api|_next/static|_next/image|favicon.ico|.*\\.).*)",
      "destination": "/index.html"
    }
  ]
}