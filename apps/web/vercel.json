{
  "framework": null,
  "buildCommand": "pnpm turbo run build --filter=apps/web...",
  "outputDirectory": "apps/web/dist",
  "redirects": [
    {
      "source": "/signup",
      "destination": "/pricing",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
