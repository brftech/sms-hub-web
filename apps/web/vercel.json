{
  "buildCommand": "pnpm build --filter @sms-hub/web",
  "outputDirectory": "dist",
  "framework": null,
  "redirects": [
    {
      "source": "/signup",
      "destination": "/pricing",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/((?!api|_next/static|_next/image|favicon.ico|.*\\.).*)",
      "destination": "/index.html"
    }
  ]
}
